                    "customer_reference": "",
                    "account_number": "898789",
                    "amount": "10",
                    "narration": "Testing",
                    "sender_info": {
                      "name": "Johnson",
                      "phone": "",
                      "email": ""
                    },
                    "receipient_info": {
                      "account_number": "22997000000",
                      "account_name": "Sam",
                      "routing_number": "123456789",
                      "account_type": "checking",
                      "receipient_address": {
                        "countryCode": "US",
