                  "currency": {
                    "type": "string"
                  },
                  "countryCode": {
                    "type": "string"
                  },
                  "payment_method": {
                    "type": "string"
                  }
                }
              },
              "examples": {
                "NGN": {
                  "summary": "NGN Transfer",
                  "value": {
                    "customer_reference": "",
                    "account_number": "898789",
                    "bank_code": "78978",
                    "amount": "10",
                    "narration": "Testing",
                    "recipient_name": "",
                    "ref_id": "sess_692824141b221c00d70876e5",
                    "sender_info": {
                      "name": "Johnson",
