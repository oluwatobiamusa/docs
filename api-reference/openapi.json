          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "customer_reference": {
                    "type": "string"
                  },
                  "account_number": {
                    "type": "string"
                  },
                  "bank_code": {
                    "type": "string"
                  },
                  "amount": {
                    "type": "string"
                  },
                  "narration": {
                    "type": "string"
                  },
                  "recipient_name": {
                    "type": "string"
                  },
                  "ref_id": {
                    "type": "string"
                  },
                  "sender_info": {
                    "type": "object",
                    "properties": {
                      "name": {
                        "type": "string"
                      },
                      "phone": {
                        "type": "string"
                      },
                      "email": {
                        "type": "string"
                      }
                    }
                  },
                  "receipient_info": {
                    "type": "object",
                    "properties": {
                      "account_number": {
                        "type": "string"
                      },
                      "account_name": {
                        "type": "string"
                      },
                      "routing_number": {
                        "type": "string"
                      },
                      "account_type": {
                        "type": "string"
                      },
                      "receipient_address": {
                        "type": "object",
